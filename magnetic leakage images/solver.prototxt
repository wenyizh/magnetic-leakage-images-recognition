#net: "train_val.prototxt"
net: "fcn11.prototxt"
display: 20
average_loss: 20
base_lr: 1e-7
lr_policy: "step"
gamma: 0.5
stepsize: 5000
# high momentum
momentum: 0.99
# no gradient accumulation
iter_size: 5
max_iter: 30000
weight_decay: 0.0005
snapshot: 5000
snapshot_prefix: "caffemodel/"
test_initialization: false
